<serial>
	<port>/dev/cu.usbmodemfd131</port>
	<baud>9600</baud>
</serial>
<capture>
	<fovMultiplier>1</fovMultiplier>
	<frameCount>60</frameCount>
	<nearClipping>40</nearClipping>
	<farClipping>95</farClipping>
</capture>
<timing>
	<fadeIn>1000</fadeIn>	
	<record>4000</record>
	<fadeOut>2000</fadeOut>
</timing>
<transfer>
	<output>
		<directory>/Users/kyle/Desktop/INCOMING_SCANS/</directory>
		<prefix>barcelona</prefix>
		<extension>png</extension>
	</output>
	<osc>
		<ip>127.0.0.1</ip>
		<port>70007</ip>
	</osc>
</transfer>